<MudNavMenu>

    <MudNavLink Href="/" Match="NavLinkMatch.All">
        <MudIcon Icon="@Icons.Material.Rounded.Dashboard" Size="Size.Medium" Style="vertical-align: middle;"  /> Dashboard
    </MudNavLink>
    <MudNavLink Href="/my-lists" Match="NavLinkMatch.Prefix">
        <MudIcon Icon="@Icons.Material.Rounded.List" Size="Size.Medium" Style="vertical-align: middle;" /> My Lists
    </MudNavLink>

    <MudNavLink Href="/all-tasks" Match="NavLinkMatch.Prefix">
        <MudIcon Icon="@Icons.Material.Rounded.CheckBoxOutlineBlank" Size="Size.Medium" Style="vertical-align: middle;" /> All Tasks
    </MudNavLink>

    <MudNavLink Href="/completed-tasks" Match="NavLinkMatch.Prefix">
        <MudIcon Icon="@Icons.Material.Rounded.CheckBox" Size="Size.Medium" Style="vertical-align: middle;" /> Completed Tasks
    </MudNavLink>

    <MudNavLink Href="/settings" Match="NavLinkMatch.Prefix">
        <MudIcon Icon="@Icons.Material.Rounded.Settings" Size="Size.Medium" Style="vertical-align: middle;" /> Settings
    </MudNavLink>

